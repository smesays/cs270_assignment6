<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>        
        <th>E-mail</th>
    </tr>
    <% @users.each do |i| %>
        <tr>
            <td><%= i.first_name %></td>
            <td><%= i.last_name %></td>
            <td><%= i.email %></td>
        </tr>
    <% end %>
</table>